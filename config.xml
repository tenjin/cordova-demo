<?xml version='1.0' encoding='utf-8'?>
<widget id="io.tenjin.cordova.demo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tenjin</name>
    <description>
        A sample Apache Cordova to demonstrate the Tenjin Cordova plugin.
    </description>
    <author email="support@tenjin.io" href="https://tenjin.io">
        Tenjin
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <!--<preference name="DisallowOverscroll" value="true" />-->
    <plugin name="cordova-plugin-x-toast" spec="~2.5.2" />
    <plugin name="cordova-plugin-console" spec="~1.0.5" />
</widget>
